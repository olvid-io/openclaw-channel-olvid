{
  "name": "olvid-channel",
  "version": "0.0.0-a4",
  "description": "Olvid native channel for OpenClaw.",
  "type": "module",
  "dependencies": {
    "@olvid/bot-node": "^1.5.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "openclaw": "2026.2.1"
  },
  "scripts": {
    "build": "npx tsc"
  },
  "keywords": [
    "openclaw",
    "channel",
    "olvid"
  ],
  "author": "bot@olvid.io",
  "openclaw": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "olvid",
      "label": "olvid",
      "selectionLabel": "Olvid (Daemon)",
      "docsPath": "/channels/olvid",
      "docsLabel": "olvid",
      "blurb": "Trust your agent with authenticated e2e encryption.",
      "order": 35
    },
    "install": {
      "npmSpec": "olvid-channel",
      "localPath": "extensions/olvid",
      "defaultChoice": "npm"
    }
  }
}
